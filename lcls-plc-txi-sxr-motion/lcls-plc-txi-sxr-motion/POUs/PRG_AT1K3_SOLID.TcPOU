<TcPlcObject Version="1.1.0.1">
  <POU Name="PRG_AT1K3_SOLID" Id="{550bc570-80c6-000a-11f2-6646ddb6249c}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM PRG_AT1K3_SOLID
VAR
    (* NOTE: use FALSE for simulation and production *)
